{
  "name": "api-schema",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate": "pnpm run generate:openapi && pnpm run generate:ts",
    "generate:openapi": "./scripts/generate-openapi.sh",
    "generate:ts": "./scripts/generate-ts.sh",
    "preview": "npx @redocly/cli preview-docs generated/openapi.yaml",
    "serve": "npx http-server generated -p 8081 -o"
  },
  "devDependencies": {
    "@typespec/compiler": "^1.8.0",
    "@typespec/http": "^1.8.0",
    "@typespec/openapi3": "^1.8.0",
    "@typespec/rest": "^0.78.0",
    "@openapitools/openapi-generator-cli": "^2.15.0",
    "typescript": "^5"
  }
}